<% include ../partials/header.ejs %>


<main class="container">


<h1> Add Client/Projects </h1>
<form method="post">
    <div class="form-group">
        <label for="firstname" class="col-sm-2 control-label">First Name: </label>
        <input name="firstname" id="name" required  />
    </div>
    <div class="form-group">
        <label for="lastname" class="col-sm-2 control-label">Last Name: </label>
        <input name="lastname" id="lastname" required  />
    </div>
            <div class="form-group">
        <label for="starttime" class="col-sm-2 control-label">starttime </label>
        <input name="starttime" id="starttime" type:"number"required/>
    </div>

    <div class="form-group">
        <label for="endtime" class="col-sm-2 control-label">endtime  </label>
        <input name="endtime" id="endtime"  type="number" min="1" max="31"  />
    </div>
    <div class="form-group">
        <label for="date" class="col-sm-2 control-label">date </label>
        <input name="date" id="date"  type="number" min="1" max="12"/>
    </div>

    <div class="form-group">
        <label for="parentname" class="col-sm-2 control-label">End Month </label>
        <input name="parentname" id="parentname"  type="text" min="1" max="12" />
    </div>
    <button class="btn btn-success col-sm-offset-2">Save</button>


</form>

</main>








