<% include ../partials/header.ejs %>


<main class="container">


    <h2 class="page-head-line">Edit Clients/Projects</h2>
    <form method="post">
        <div class="form-group">
            <label for="firstname" class="col-sm-2 control-label">First Name: </label>
            <input name="firstname" id="name" required  value="<%= list.firstname %>" />
        </div>
        <div class="form-group">
            <label for="lastname" class="col-sm-2 control-label">Last Name: </label>
            <input name="lastname" id="lastname" required  value="<%= list.lastname %>" />
        </div>
                <div class="form-group">
            <label for="starttime" class="col-sm-2 control-label">starttime </label>
            <input name="starttime" id="starttime" type:"number"required  value="<%= list.starttime %>" />
        </div>

        <div class="form-group">
            <label for="endtime" class="col-sm-2 control-label">endtime  </label>
            <input name="endtime" id="endtime"  type="number" min="1" max="31"  value="<%= list.endtime %>" />
        </div>
        <div class="form-group">
            <label for="date" class="col-sm-2 control-label">date </label>
            <input name="date" id="date"  type="number" min="1" max="12" value="<%= list.date %>" />
        </div>

        <div class="form-group">
            <label for="parentname" class="col-sm-2 control-label">End Month </label>
            <input name="parentname" id="parentname"  type="text" min="1" max="12" value="<%= list.parentname %>" />
        </div>
        <button class="btn btn-success col-sm-offset-2">Save</button>


    </form>

</main>








<% include ../partials/footer.ejs %>
